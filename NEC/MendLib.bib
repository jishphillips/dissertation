@article{Mushin2007,
abstract = {Overlap in conversation is a well-established area of conversation analysis research (e.g. Jefferson 1983, Schegloff 2000) which can reveal how participants orient to transition relevance places. This paper presents an analysis of overlap in the mixed (Garrwa, Kriol and English) language conversations of two indigenous Australian women as part of a larger study of turn-taking practices in indigenous conversations. Walsh (Walsh 1995) made some observations about Aboriginal conversational style, for example that they may enter a conversation without attending to the talk of others. His observational claims are empirically examined here in the context of our data. We find that the overlapping talk in our data follows many patterns similar to English speakers' talk, including transition space overlap (cf. Jefferson 1983) and simultaneous starts. The most important difference we found was overlap onset occurring shortly after the closure of the transition space, reflecting disattendance by speakers to the content, but not the timing, of each other's talk. Overall, however, we find that the turn-taking of these two women is overwhelmingly orderly, and deviations from orderliness can mostly be accounted for by their orientation to points of possible completion and rules of turn-taking as described by Sacks, Schegloff and Jefferson (1974).},
author = {Mushin, Ilana and Gardner, Rod},
doi = {10.2104/aral0735},
issn = {0155-0640},
journal = {Australian Review of Applied Linguistics},
pages = {35.1--35.1},
title = {{Post-start up overlap and disattentiveness in talk in a Garrwa community}},
url = {http://www.linguisticsabstracts.com/AbstractDetail.aspx?id=50270},
volume = {30},
year = {2007}
}
@article{McElree2001a,
abstract = {Verbs like ‘begin' and ‘enjoy' appear to semantically select a complement that expresses an activity or an event (Jackendoff, R. (1997). The architecture of the language faculty. Cambridge, MA: MIT Press; Pustejovsky, J. Cognition 41 (1991) 47; Pustejovsky, J. (1995). The generative lexicon. Cambridge, MA: MIT Press). When these verbs occur with a complement that does not directly denote either an activity or an event (e.g. ‘{\ldots}began the book' or ‘{\ldots}enjoyed the book'), the complement must be type-shifted from an object to an activity to conform to the verb's semantic restrictions. We report an experiment in which self-paced reading times were found to be longer for complements that required type-shifting than for complements that directly matched the semantic restrictions of the matrix verb. This finding is argued to provide on-line evidence for a type of enriched lexical processing posited in recent lexical semantic research.},
author = {McElree, Brian and Traxler, Matthew J and Pickering, Martin J and Seely, Rachel E and Jackendoff, Ray},
doi = {http://dx.doi.org/10.1016/S0010-0277(00)00113-X},
issn = {0010-0277},
journal = {Cognition},
keywords = {Reading time Evidence Enriched composition},
pages = {B17--B25},
title = {{Reading time evidence for enriched composition}},
url = {internal-pdf://179.58.177.4/McElree et al 2001 Cognition.pdf http://www.sciencedirect.com/science/article/pii/S001002770000113X},
volume = {78},
year = {2001}
}
