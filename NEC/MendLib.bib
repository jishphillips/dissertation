@book{Chung2004,
abstract = {This vol in the Linguistic Inquiry Monographs series addresses the interaction of syntactic structure {\&} semantic interpretation in the case of indefinites, understood in the sense of Hans Kamp (1981) {\&} Irene Heim as restricted free variables. Two modes of composition or styles of introduction are distinguished in the semantics {\&} syntax of indefinites: a predicate-argument relation that saturates the predicate {\&} a nonsaturating mode of predicate restriction. This theory predicts that a specific language can use distinctive formal means to specify which mode of composition applies to any particular indefinite; the prediction is borne out by an analysis of the strongly felt contrast between two Maori indefinite articles he {\&} tetahi, which signal composition by restriction {\&} specification respectively. Another key prediction of the theory, that a restricted argument remains available for semantic composition by saturation or futher restriction, is tested successfully in an analysis of object incorporation in Chamorro. 1 Bibliog, 2 Appendixes. J. Hitchcock},
address = {Cambridge, MA},
author = {Chung, Sandra and Ladusaw, William A},
booktitle = {Linguistic Inquiry},
