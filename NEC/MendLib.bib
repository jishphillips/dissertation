@article{McWhorter1998,
abstract = {An increasingly influential current in creole studies considers there to have been no appreciable break in transmission or simplification in the birth of creole languages, instead treating creoles as gradual, relatively nondisruptive developments of their lexifiers amidst conditions of heavy language contact. Central to this superstratist view is the claim that creole is a sociohistorical, but not synchronic, term. This article outlines three features which in fact render creoles synchronically distinguishable from other languages, all three of them clear results of a break in transmission followed by a development period too brief for the traits to be undone as they have been in older languages. It is also shown that an expanded data set reveals flaws in the sociohistorical argumentation behind the superstratist framework.},
author = {McWhorter, John H},
issn = {00978507},
journal = {Language},
pages = {788--818},
title = {{Identifying the Creole Prototype: Vindicating a Typological Class}},
url = {http://www.jstor.org/stable/417003},
volume = {74},
year = {1998}
}
