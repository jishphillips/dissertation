@article{Amery1993,
abstract = {Dhuwaya, spoken in Yirrkala (Northeast Arnhemland, Australia), is cited as an example of koineization in an Australian language. Unlike the adoption of a creole, which is a language shift, the adoption of a koine is a dialect shift. This dialect shift is still in progress in Yirrkala. Dhuwaya is a variety of Yol(eng)u Matha (YM) that has been the result of language contact between different dialects of YM, evolving as a common dialect between the young of various clans. The differences among dialects, of which there are approximately 15, are discussed {\&} the features of Dhuwaya are described; three fairly distinct varieties exist - Baby Dhuwaya (baby talk), Developmental Dhuwaya (child language), {\&} Adult Dhuwaya. Dhuwaya morphology differs in four main ways from other dialects. The development of Dhuwaya is traced {\&} its place in Yirrkala society discussed. Speculation is offered as to the extent that traditional clan dialects will be maintained, given the existence of Dhuwaya; the development of a stable diglossia where both the clan dialects {\&} Dhuwaya have distinct roles {\&} functions could be the deciding factor. Although there is strong outward opposition to Dhuwaya, it is strongly entrenched within the society of Yirrkala {\&} becoming even stronger. 2 Tables, 29 References. V. Patterson},
author = {Amery, Rob},
doi = {10.1515/ijsl.1993.99.45},
