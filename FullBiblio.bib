journal = {Linguistic Typology},
keywords = {Counterfactuality,Gricean maxims,Implicature,Irrealis,Modality,Mood,Negation,Non-Pama-Nyungan,Tense},
number = {2},
pages = {223--268},
title = {{The semantics and pragmatics of composite mood marking: The non-Pama-Nyungan languages of northern Australia}},
volume = {9},
year = {2005}
}
