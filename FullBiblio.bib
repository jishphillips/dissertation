@article{Kamp1971,
abstract = {This paper is a development of an earlier paper by A. N. Prior [Nous$\backslash$n2 (1968), 101--119]. A semantical framework is described in which$\backslash$ntense logics can be extended to include a unary propositional connective$\backslash$n{\{}$\backslash$bf N{\}} corresponding to the function of "now" in ordinary discourse.$\backslash$nA number of results are established, the most important of which$\backslash$nare of the following form: Let $\backslash$scr L({\{}N{\}}) be a tense logic with$\backslash$nthe "now"-operator and let $\backslash$scr L be the calculus obtained by omitting$\backslash$n{\{}N{\}} from $\backslash$scr L({\{}N{\}}); if an axiom system $\backslash$scr A for $\backslash$scr L$\backslash$nis semantically complete, then so is an axiom system $\backslash$scr A' for$\backslash$n$\backslash$scr L({\{}N{\}}), where $\backslash$scr A' is closely related to $\backslash$scr A. The$\backslash$npaper is in two parts, the first restricted to propositional calculi,$\backslash$nwhere it is shown that the "now"-operator is always eliminable, while$\backslash$nthe second is devoted to (first order) predicate calculi (with identity),$\backslash$nwhere it is shown that the "now"-operator is not always eliminable.$\backslash$nIf $\backslash$scr T=$\backslash$langle T,{\textless}$\backslash$rangle represents the structure of time$\backslash$n(which for the most part the author assumes to be a partial ordering)$\backslash$nand $\backslash$germ M is an interpretation relative to $\backslash$scr T, the obvious$\backslash$ncharacterization of the truth-value of a formula $\phi$ in $\backslash$germ$\backslash$nM at a moment t$\backslash$in T is shown to require an additional parameter$\backslash$nin order to be extended to an appropriate semantical clause for the$\backslash$n"now"-operator. This is essentially because the word "now" when used$\backslash$nin ordinary discourse refers back to the moment of utterance, in$\backslash$nwhich case the concept to be characterized is that of the truth-value$\backslash$nof $\phi$ in $\backslash$germ M at t when part of an expression uttered$\backslash$nat t'. A formula is then to be regarded as valid (relative to $\backslash$scr$\backslash$nT) if in every interpretation it is true when uttered. The author$\backslash$nalso defines and uses alternative but equivalent notions in which$\backslash$nan interpretation is a pair $\backslash$langle$\backslash$germ M,t$\backslash$sb 0$\backslash$rangle, where$\backslash$n$\backslash$germ M is as before and t$\backslash$sb 0 represents the present moment.$\backslash$nIn addition, instead of restricting questions of completeness to$\backslash$na single temporal structure $\backslash$scr T, the author considers non-empty$\backslash$nclasses of partial orderings and characterizes notions of strong$\backslash$nand weak {\{}K{\}}-completeness. He also shows that some axiom systems$\backslash$nare weakly but not strongly {\{}$\backslash$bf K{\}}-complete, for appropriate classes$\backslash$n{\{}K{\}}. Finally, it should also be noted that the results described$\backslash$nin this paper apply to a rather general notion of a tense operator.$\backslash$nUnlike truth-functions, the relevant arguments and values of a tense$\backslash$noperator are not truth-values but "courses of truth-values through$\backslash$ntime", i.e., functions from moments to truth-values.},
author = {Kamp, Hans},
doi = {10.1163/9789004252882_004},
file = {:Users/joshphillips/Documents/Mendeley/j.1755-2567.1971.tb00071.x.pdf:pdf},
journal = {Theoria},
number = {3},
pages = {227--73},
title = {{Formal Properties of ‘Now'}},
volume = {37},
year = {1971}
}
