@article{Fleischman1989,
abstract = {It is conventionally agreed that the basic function of tense categories is to locate events in time relative to a reference point, which in most tense systems is the "now" of the speaker. Although languages vary in the extent to which they grammaticalize degrees of distance from this reference point ("remoteness distinctions"), the notion of temporal distance nonetheless functions across languages as a prominent metaphorical template for the expression of distance along other conceptual or cognitive axes that receive linguistic coding in particular languages. By presenting relevant data from a range of (primarily but not exclusively Indo-European) languages, it is demonstrated how the temporal distance concept, as expressed specifically via tense categories, is systematically extended, synchronically {\&} diachronically, to convey distance along several other axes, both grammatical {\&} pragmatic: axes of modality (epistemic {\&} deontic), assertiveness, social/interpersonal distance (politeness, hypocoristic forms), evidentiality, {\&} speaker subjectivity. 9 Figures, 88 References. AA},
annote = {http://covers-cdn.summon.serialssolutions.com/index.aspx?isbn=/sc.gif{\&}issn=0378-4177{\&}client=summon;http://covers-cdn.summon.serialssolutions.com/index.aspx?isbn=/mc.gif{\&}issn=0378-4177{\&}client=summon;http://covers-cdn.summon.serialssolutions.com/index.aspx?isbn=/lc.gif{\&}issn=0378-4177{\&}client=summon; U6 - ctx{\_}ver=Z39.88-2004{\&}ctx{\_}enc=info{\%}3Aofi{\%}2Fenc{\%}3AUTF-8{\&}rfr{\_}id=info{\%}3Asid{\%}2Fsummon.serialssolutions.com{\&}rft{\_}val{\_}fmt=info{\%}3Aofi{\%}2Ffmt{\%}3Akev{\%}3Amtx{\%}3Ajournal{\&}rft.genre=article{\&}rft.atitle=TEMPORAL+DISTANCE+-+A+BASIC+LINGUISTIC+METAPHOR{\&}rft.jtitle=STUDIES+IN+LANGUAGE{\&}rft.au=FLEISCHMAN{\%}2C+S{\&}rft.date=1989-01-01{\&}rft.pub=JOHN+BENJAMINS+B+V+PUBL{\&}rft.issn=0378-4177{\&}rft.volume=13{\&}rft.issue=1{\&}rft.spage=1{\&}rft.epage=50{\&}rft.externalDBID=n{\%}2Fa{\&}rft.externalDocID=A1989AL85300001{\&}paramdict=en-US U7 - Journal Article},
