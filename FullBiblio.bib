@article{Hobbs2005,
abstract = {We do things in the world by exploiting our knowledge$\backslash$nof what causes what. But in trying to reason formally about$\backslash$ncausality, there is a difficulty: to reason with certainty we$\backslash$nneed complete knowledge of all the relevant events and$\backslash$ncircumstances, whereas in everyday reasoning tasks we need a more$\backslash$nserviceable but looser notion that does not make such demands on$\backslash$nour knowledge. In this work the notion of causal complex is$\backslash$nintroduced for a complete set of events and conditions necessary$\backslash$nfor the causal consequent to occur, and the term cause is used$\backslash$nfor the makeshift, nonmonotonic notion we require for everyday$\backslash$ntasks such as planning and language understanding. Like all$\backslash$ninteresting concepts, neither of these can be defined with$\backslash$nnecessary and sufficient conditions, but they can be more or less$\backslash$ntightly constrained by necessary conditions or sufficient$\backslash$nconditions. The issue of how to distinguish between what is in a$\backslash$ncausal complex from what is outside it is discussed, and within a$\backslash$ncausal complex, how to distinguish the eventualities that deserve$\backslash$nto be called causes from those that do not, in particular$\backslash$ncircumstances. One particular modal, the word 'would', is examined$\backslash$nfrom the standpoint of its underlying causal content, as a$\backslash$nlinguistic motivation for this enterprise.},
author = {Hobbs, Jerry R.},
doi = {10.1093/jos/ffh024},
file = {:Users/joshphillips/Documents/Mendeley/Hobbs05.pdf:pdf},
issn = {01675133},
journal = {Journal of Semantics},
number = {2},
pages = {181--209},
title = {{Toward a useful concept of causality for lexical semantics}},
volume = {22},
year = {2005}
}
