@article{Blevins2006,
abstract = {Disparities in the distribution of specific sound patterns across languages of the world are investigated in the theoretical framework of Blevins's evolutionary phonology, which views language as a complex adaptive system with useful parallels in biological evolution. Five general sources of biological {\&} linguistic similarity are posited: (1) direct genetic inheritance; (2) independent development due to adaptation, whether through selective advantage or phonetic motivation; (3) physical constraints; (4) chance; {\&} (5) external factors that include genetic modification {\&} hybridization in biology {\&} language contact, prescriptive norms, literacy, {\&} second-language learning in linguistics. Selective mechanisms of change, chance, {\&} choice operate in the transmission of language through speech perception {\&} misperception. Extending {\&} diverging from neogrammarian views of sound change {\&} Greenbergian typology, evolutionary phonology differs from generative phonology {\&} optimality theory in prioritizing explanations that are external to phonology over competing phonological explanations. The framework is applied to develop an account of final devoicing as an emergent property of phonological systems that does not arise from any innate universal; evolutionary phonology predicts the high frequency of final devoicing {\&} relative rarity of final voicing, recurrent stages of genesis of final devoicing, {\&} its phonologized domains. Tables, References. J. Hitchcock},
author = {Blevins, Juliette},
