@article{Comrie1981,
abstract = {In the Saibai dialect of Kalaw Lagaw Ya, an Australian lang spoken on the Torres Strait Islands, NPs exhibit four case-marking systems for nuclear grammatical relations (intransitive subject = S, agent = A, {\&} patient = P of transitive constructions): (1) singular pronouns have different forms for each S, A, {\&} P; (2) nonplural common Ns have one for A (ergative) {\&} another for S {\&} P (absolutive); (3) nonplural proper names {\&} kinship terms of address have one form for S {\&} A (nominative) {\&} another for P (accusative); {\&} (4) nonsingular pronouns {\&} plural common Ns have a single form for all S, A, {\&} P. Syntactic processes in Saibai also group S, A, {\&} P in different ways, but completely independently of NP case marking: V agreement operates ergative-accusatively (Vs agree with S {\&} P). Case marking in nonfinite constructions uses the same case, dative, for all S, A, {\&} P. Imperative addressee deletion {\&} infinitive subject deletion operate nominative-accusatively (S {\&} A, but not P, can be deleted). Nonfinite relative clauses can be formed where the head N functions as S, A, or P of the relative clause, though with one set of forms for S {\&} A, {\&} another set for P. Saibai provides an excellent illustration of mixed ergative-accusative morphology {\&} syntax, {\&} of the irrelevance of morphology for major syntactic processes. 2 Tables. AA},
author = {Comrie, Bernard},
doi = {10.1080/07268608108599265},
file = {:Users/joshphillips/Documents/Mendeley/Comrie - 1981.pdf:pdf},
isbn = {0726-8602, 0726-8602},
issn = {14692996},
journal = {Australian Journal of Linguistics},
number = {1},
pages = {1--42},
title = {{Ergativity and Grammatical Relations in Kalaw Lagaw Ya (Saibai Dialect).}},
volume = {1},
year = {1981}
