@article{Grammar1975,
abstract = {A theory of grammar (syntax {\&} semantics) is proposed combining the most essential feature of Richard Montague's theory of syntax {\&} semantics {\&} the transformational approach to syntax. The resulting theory is closer to Montague than to any of the current versions of transformational grammar; nevertheless, close analogues of most of the rules (both phrase structure {\&} transformational) formulated by transformationalists can be naturally included in the new theory. Montague's central working premise, carried over here, is that the syntactic rules determining how a sentence is built out of smaller syntactic parts should correspond 1-to-1 with the semantic rules telling how the meaning of a sentence is a function of the meanings of its parts. This idea is not new in either linguistics or philosophy - in philosophy it has its basis in the work of Frege, Tarski, {\&} Carnap; it is also standard in the treatment of formalized languages; {\&} in transformational grammar it can be found in Katz {\&} Fodor's earliest work on semantics (1963). However, with a few exceptions (e.g., Reichenbach, 1947), philosophers before Montague have applied this principle only to describing constructed languages {\&} have sometimes expressed skepticism as to whether natural languages would even admit of such a description. Linguists, on the other hand, have attempted to apply principles of syntactic-semantic correspondence to natural languages, but their definitions of semantic theory have diverged considerably from the philosophico-logical tradition. Alternative conceptions of semantics are proposed, {\&} the suggestion that there is no principled reason for linguists to eschew the philosophico-logical conception of semantics is proposed on the basis that there are in fact some good reasons for adopting it. The 2nd {\&} largest section of the article - primarily for linguists - is devoted to an explication of Montague's approach to syntax {\&} semantics. In the 3rd section - primarily addressed to philosophers - main features of transformational syntax {\&} problems raised in various attempts to add semantics to the theory are discussed. HA},
author = {Partee, Barbara},
file = {:Users/joshphillips/Documents/Mendeley/Partee - 1975.pdf:pdf},
issn = {00243892},
journal = {Linguistic Inquiry},
number = {2},
pages = {203--300},
title = {{Montague grammar and transformational grammar}},
volume = {6},
year = {1975}
}
@incollection{Botne2012,
abstract = {A small but significant number of the world's languages have the capacity to express grammatically not only simple tense relations of past and future, but also finer distinctions indicating the distance or "degree of remoteness" from the deictic center, typically the time of utterance. This capacity to express grammatically various degrees of remoteness, whether temporal or modal, constitutes an important dimension of the tense-aspect-mood systems in these languages. This article examines the common bases for delineating temporal intervals-natural cyclic divisions, human life cycle and memory, and epistemic value-and also discusses the concept of remoteness, the typical organization of multi-tense systems, and the complexity of such systems. Moreover, it summarizes three natural daily cycles that serve to organize multi-tense systems: today (Hodiernal), with its complementary counterparts yesterday (Hesternal) and tomorrow (Crastinal); a 24-hour cycle (Diurnal); and a conflation of today and one-day-away (Biduonal). Finally, the article considers complex systems and remoteness, time scales, and deictic centers.},
author = {Botne, Robert},
booktitle = {The Oxford Handbook of Tense and Aspect},
doi = {10.1093/oxfordhb/9780195381979.013.0018},
file = {:Users/joshphillips/Documents/Mendeley/Botne - 2012(2).pdf:pdf},
isbn = {9780199940875},
keywords = {Complex systems,Deictic centers,Epistemic value,Human life cycle,Memory,Natural daily cycles,Remoteness,Temporal intervals,Tense,Time scales},
pages = {536--60},
title = {{Remoteness Distinctions}},
year = {2012}
}
