@article{Oversteegen2005,
abstract = {By force of {\textless}it{\textgreater}causes precede effects{\textless}/it{\textgreater}, causality contributes to the temporal meaning of discourse. In case of semantic causal relations, this contribution is straightforward, but in case of epistemic causal relations, it is not. In order to gain insight into the semantics of epistemic causal relations, paradoxical cases are analyzed of text fragments in which temporal {\&} causal meaning seem to be irreconcilable. A solution is proposed consisting of several parts: First, an analysis of epistemic causal coherence relations, in which the effect sentence is argued to be a (covert) belief sentence. Secondly, a method for deriving (epistemic) causal coherence relations. Thirdly, a format for representing temporal meaning in which contributions of tense {\&} causality can be reconciled. Finally, it is argued that understanding epistemically causally connected sequences (or: argumentation) is not equal to being persuaded to believing them. The difference is shown to be expressible in the representation format proposed. 1 Table, 50 References. Adapted from the source document},
author = {Oversteegen, Leonoor},
