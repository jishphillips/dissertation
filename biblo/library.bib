@article{Nichols1986,
abstract = {Morphological marking ofgrammatical relations may appear on either the head or the dependent member ofthe constituent (or on both, or on neither). Grammatical relations— and whole languages—may be classified according to their propensity for using one of these types of marking. Implicational relations among various marking patterns can be stated: languages display a tendency to use one type consistently throughout their gram- mar. The difference in patterns provides a typological metric and a functional explanation for certain word-order preferences. For historical linguistics, it provides a diagnostically conservative feature and a clue to genetic relatedness. Although the head-marked pattern is cross-linguistically favored, grammatical theory is strongly biased toward the depen- dent-marked patterns that happen to dominate in Indo-European},
