@article{Geurts2018,
abstract = {Conventions are regularities in social behaviour of the past that enable us to coordinate our actions. Some conventions are lawlike: they are expected to be observed always or nearly always. However, in order to coordinate our actions, it may suffice that a precedent has occurred often enough, and sometimes even a single precedent will do. So, in general, conventions merely enable us to solve our coordination problems; lawlike conventions are a special case. Grammatical conventions are often lawlike; sense conventions are typically enabling. In order to resolve the indeterminacies that sense conventions give rise to, interlocutors must rely on the common ground. In this and other ways, common ground is a prerequisite for convention-based communication.},
author = {Geurts, Bart},
doi = {10.1111/mila.12171},
