journal = {Australian Journal of Linguistics},
keywords = {Case-marking,Gunwinyguan,Information Structure,Nominal Reference,Optional Ergative,Topicality,Transitivity},
number = {3},
pages = {287--328},
publisher = {Taylor {\&} Francis},
title = {{Discourse and Pragmatic Functions of the Dalabon ‘Ergative' Case-marker}},
volume = {39},
year = {2019}
}
