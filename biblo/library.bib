@inproceedings{Bowern1998,
abstract = {Modality and irrealis are two terms which are rather ill-defined. The classification of a modality as either deontic and epistemic is not sufficiently narrow to reflect the nuances of meaning in different languages; nor is a morphological distinction between these two parameters frequently expressed in languages (cf Roberts (1990;368)). Conversely, other definitions contain supposedly exhaustive lists of possible modalities and disguise the fact that the majority of these are found very seldom. Moreover, little has been written on the way in which modality and the marking of irreality interact. It seems generally assumed that the marking of a modality as irrealis occurs fairly much at random. In this paper it will be shown that modality may be defined along six parameters. Languages occasionally make further distinctions within these categories, yet since these labels represent grammaticalised 'speaker attitudes' to propositions, not all languages grammaticalise every category. It will also be demonstrated that the relation between irrealis marking and modality in a language follows a hierarchy, and the marking of a particular modality as irrealis may be predicted.},
author = {Bowern, Claire},
booktitle = {Selected papers from the 1998 Conference of the Australian Linguistic Society},
editor = {Ingram, John},
file = {:Users/joshphillips/Documents/Mendeley/Bowern - 1998.pdf:pdf},
title = {{Towards a typology of irrealis marking in modality}},
