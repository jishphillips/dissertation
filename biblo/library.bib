@article{Starr2014,
abstract = {The usual semantics for material conditionals are strikingly different from those for strict and variably strict conditionals. But using the approach of the theory of rational belief systems developed in [I] a unified account of these three kinds of conditionals can be given. It is possible to define a general conditional, and to exhibit the three kinds as special cases. The general theory is that a conditional ‘$\alpha$ → $\beta$' is accepted as true in a rational belief system B only if the belief that ‘p is false does not occur in any completed extension of a belief system BA which is obtained from B in some way by making the supposition that B'$\alpha$. The kind of conditional in question depends on how B'$\alpha$ is defined in relation to B. In the second section of the paper, I take up the question of how this general theory of conditionals should be applied. I consider but reject the view that the indicative conditionals of English are normally material conditionals, and argue that indicative and subjunctive conditionals are usually variant locutions for the one kind of conditional which is variably strict. I claim that for every subjunctive conditional there is a corresponding indicative and that where this is so, the indicatives and their corresponding subjunctives express the same beliefs, but do so in different ways and against different backgrounds of knowledge or belief. The theory at once resolves those paradoxes of material conditionals which cannot be seen to depend upon any confusions of truth with assertability conditions and provides a framework for explaining how the use of subjunctive conditionals comes to be learned and understood. On ‘possible worlds' semantics with its hierarchical structure of similarity relationships between mutually inaccessible possible worlds, this remains a mystery.},
author = {Starr, William B},
doi = {10.1007/s10992-013-9300-8},
file = {:Users/joshphillips/Documents/Mendeley/Starr - 2014.pdf:pdf},
issn = {15730433},
journal = {Journal of Philosophical Logic},
keywords = {Conditionals,Dynamic semantics,Logic,Meaning},
number = {6},
pages = {1019--1064},
title = {{A Uniform Theory of Conditionals}},
volume = {43},
year = {2014}
}
