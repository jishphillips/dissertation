file = {:Users/joshphillips/Documents/Mendeley/Partee, Hendriks - 2011.pdf:pdf},
isbn = {9780444537263},
keywords = {Anaphora,Formal semantics,Intensionality,Model theory,Montague grammar,Quantification,Type theory},
pages = {3--94},
publisher = {Elvesier},
title = {{Montague Grammar}},
year = {2011}
