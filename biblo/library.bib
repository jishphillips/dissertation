@article{Beck2012,
abstract = {In this paper, we provide a uni ied semantics for the Classical Greek particle $\alpha$$\nu$ in its uses both in and outside of conditional sentences. Specifically, working within the framework provided by formal semantic treatments of conditionals in Stalnaker (1968); Lewis (1973); Kratzer (1981) and subsequent work, we propose that $\alpha$$\nu$ is a universal quantifier over situations-parts of possible worlds. We also detail the interactions between $\alpha$$\nu$ and the tense and mood features in a clause, arguing, for example, that the semantics of $\alpha$$\nu$ in combination with a 'fake' past tense morphology (Iatridou 2000), which reflects the presence of an exclusion feature in C, gives rise to a counterfactual implicature. Additionally, we address the issue of the surface distribution of $\alpha$$\nu$ in the antecedents of some types of conditionals and the consequents of others and argue that, despite its surface distribution, $\alpha$$\nu$ is always merged into the consequent of a conditional but sometimes undergoes displacement such that it appears to be located within the antecedent. Our proposal not only illuminates a complex phenomenon in Classical Greek, but also contributes to the understanding of the morpho-semantics of mood, conditionals, and counterfactuality in natural language. {\textcopyright} 2012 BRILL.},
author = {Beck, Jana E and Malamud, Sophia A and Osadcha, Iryna},
doi = {10.1163/156658412X649986},
file = {:Users/joshphillips/Documents/Mendeley/Beck, Malamud, Osadcha - 2012.pdf:pdf},
issn = {15665844},
journal = {Journal of Greek Linguistics},
keywords = {Classical Greek,conditionals,counterfactuals,cross-world situations,fake past tense,modality},
number = {1},
pages = {51--83},
title = {{A semantics for the particle ἄ$\nu$ in and outside conditionals in Classical Greek}},
volume = {12},
year = {2012}
}
